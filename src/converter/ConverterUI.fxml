<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane prefHeight="48.0" prefWidth="822.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="converter.ConverterController">
   <children>
      <TextField fx:id="leftText" onAction="#onEnterLeft" prefHeight="25.0" prefWidth="164.0">
         <FlowPane.margin>
            <Insets left="35.0" right="5.0" top="10.0" />
         </FlowPane.margin></TextField>
      <ComboBox fx:id="choiceChoose" prefHeight="25.0" prefWidth="130.0">
         <FlowPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </FlowPane.margin>
      </ComboBox>
      <Label text="=">
         <FlowPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </FlowPane.margin>
      </Label>
      <TextField fx:id="rightText" onAction="#onEnterRight" prefHeight="25.0" prefWidth="164.0">
         <FlowPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </FlowPane.margin></TextField>
      <ComboBox fx:id="choiceConvert" prefHeight="25.0" prefWidth="130.0">
         <FlowPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </FlowPane.margin>
      </ComboBox>
      <Button fx:id="convertButton" mnemonicParsing="false" onAction="#handleCovert" text="Convert">
         <FlowPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </FlowPane.margin></Button>
      <Button fx:id="clearButton" mnemonicParsing="false" onAction="#handleClear" prefWidth="51.0" text="Clear">
         <FlowPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </FlowPane.margin></Button>
   </children>
</FlowPane>
